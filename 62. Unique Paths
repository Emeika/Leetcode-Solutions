def uniquePaths(self, m: int, n: int) -> int:
    dp = [[1] * n for _ in range(m)]
    # Start from the second row and second column
    for i in range(1, m):
        for j in range(1, n):
            # Number of paths to cell (i, j) is the sum of paths from above and left
            dp[i][j] = dp[i - 1][j] + dp[i][j - 1]
    # The value in dp[m-1][n-1] contains the number of unique paths to the bottom-right corner
    return dp[m - 1][n - 1]
